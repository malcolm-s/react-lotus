{
  "name": "react-lotus",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "react-scripts": "0.2.1",
    "tap-dot": "^1.0.5",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "rxjs": "^5.0.0-beta.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "tape --require babel-register src/**/*Fixture.js | tap-dot",
    "test:watch": "watch --color \"tape --require babel-register src/**/*Fixture.js | tap-dot\""
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js"
  }
}
